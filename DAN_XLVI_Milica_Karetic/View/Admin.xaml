<Window x:Class="DAN_XLVI_Milica_Karetic.View.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DAN_XLVI_Milica_Karetic.View"
        mc:Ignorable="d"
        Background="Gray"
        Title="Admin" Height="450" Width="450">
    <Grid>
        <Button Name="btnAddManager"
                Content="Add Manager"
                Height="20"
                Width="80"
                HorizontalAlignment="Left"
                Command="{Binding AddManager, UpdateSourceTrigger=PropertyChanged}"
                Margin="20,0,0,0" />
        <Button Name="btnLogOff"
                Content="Log Off"
                Height="20"
                Width="70"
                HorizontalAlignment="Left"
                Command="{Binding Logoff, UpdateSourceTrigger=PropertyChanged}"
                Margin="20,0,0,80"
                 />
    </Grid>
</Window>
